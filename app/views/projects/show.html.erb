<h1><%= link_to @project.team.title, team_url(@project.team.id) %><h1>
<h2>Project: <%= @project.title %></h2>
<h3>Description:<h3>
<p><%= @project.description %><p>
<br><br>
<h3>Items<h3>

    <% @project.items.each do |item| %>
    <div>
      <%= link_to item.title, item_url(item.id)%>
      <% form_for item do |f|%>
      <%= check_box_tag 'completed[]', item.completed %>
      <% end %>
    </div>
<% end%>

<%= link_to "Project Items", project_items_url(@project.id)%>


<%= link_to "Add project item", new_item_url(:project_id => @project.id)%>

<%= link_to "Edit this project", edit_project_url(@project.id)%>
