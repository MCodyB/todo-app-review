<%= form_for([@team, @project], html: {multipart: true}) do |f| %>

  <%= f.label(:title, "Project Title:")%>
  <%= f.text_field(:title)%><br>

  <%= f.label(:description, "Description:")%><br>
  <%= f.text_area(:description)%>
  <br>
	 <%= f.select :team_id, Team.all.map { |team| [team.title, team.id] }, :selected => params[:team_id] %>
  <br>

  <%= f.submit%>

<% end %>